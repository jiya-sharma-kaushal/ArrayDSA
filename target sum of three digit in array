public class targetsumthree {
    public static void main(String[] args) {

        int arr[] ={1,4,2,6,5};
        int n=arr.length;
        int ans =0;
        for(int i=0;i<n;i++){
            for (int j = i + 1; j < n; j++) {
                for (int k = j + 1; k < n; k++) {
                    if (arr[i] + arr[j] + arr[k] == 13) {
                        ans++;
                    }
                }
            }
        }
        System.out.println(ans);
    }
}
